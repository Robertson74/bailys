lightGray = #cccccc
mediumGray = #adadad
darkGray = #777777
darkestGray = #222222
headingFont = adobe-caslon-pro

body
    background-color darkestGray
    color mediumGray

hr
  border-color darkGray

//@font-face
    //font-family: Caslon
    //src: url(../fonts/acaslonpro-regular.otf)
a:hover
  text-decoration none

h1, h2, h3, h4, h5, h6
  color darkGray
  font-family headingFont
  font-variant small-caps

.thumbnail
  background-color darkestGray
  border none
  max-width 300px

.thumbnail img
  border solid 1px darkGray
  padding 0px

.row1
  background-color mediumGray

.row1 p
  color darkGray

.row2
  background-color darkGray

.caption p
  color mediumGray

.caption a h3
  color mediumGray

.captionJump
  font-family adobe-caslon-pro
  font-variant small-caps

#logo
    font-family: adobe-caslon-pro
    font-size: 2em

#myNav
    border-radius 0
    margin-bottom 0

#myJumbo
    border-radius 0
    margin-top -20px
    padding 0

.navbar-brand
    padding 20px 15px

